{
	"plugins": ["plugins/markdown"],
	"recurseDepth": 10,
	"source": {
		"include": ["src"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/",
		"template": "./custom-template",
		"readme": "./README.md"
	}
}
